{
  "name": "meme-gallery-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "A basic Express API with GET/POST /memes routes, now using Prisma + PostgreSQL.",
  "scripts": {
    "start": "node index.js",
    "seed": "node prisma/seed.js",
    "db:seed": "npx prisma db seed",
    "db:generate": "npx prisma generate",
    "db:pull": "npx prisma db pull",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:status": "npx prisma migrate status",
    "db:studio": "npx prisma studio",
    "test": "cross-env NODE_ENV=test node --test"
  },
  "keywords": [
    "express",
    "api",
    "memes",
    "node",
    "prisma",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "bcrypt": "^6.0.0",
    "express": "^4.19.2",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "prisma": "^6.17.1",
    "supertest": "^7.1.4"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
